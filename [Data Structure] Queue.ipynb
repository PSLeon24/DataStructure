   "source": [
    "from collections import deque\n",
    "que = deque()\n",
    "\n",
    "# 삽입(5) - 삽입(2) - 삽입(3) - 삽입(7) - 삭제() - 삽입(1) - 삽입(4) - 삭제()\n",
    "que.append(5)\n",
    "que.append(2)\n",
    "que.append(3)\n",
    "que.append(7)\n",
    "que.popleft()\n",
    "que.append(1)\n",
    "que.append(4)\n",
    "que.popleft()\n",
    "\n",
    "print(que)\n",
    "que.reverse() # reverse\n",
    "print(que)"
   ]
  },
